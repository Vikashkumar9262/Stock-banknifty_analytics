# BankNifty Analytics Frontend Environment Variables

# Backend Configuration
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_BACKEND_WS_URL=ws://localhost:8000/ws

# GitHub Configuration
REACT_APP_GITHUB_URL=https://github.com/yourusername/banknifty-analytics
REACT_APP_GITHUB_REPO=yourusername/banknifty-analytics

# Application Configuration
REACT_APP_NAME=BankNifty Analytics
REACT_APP_VERSION=0.1.0
REACT_APP_DESCRIPTION=Real-time financial data analytics platform

# Feature Flags
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_PYTHON_BACKEND=true
REACT_APP_ENABLE_FALLBACK=true

# Analytics (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_MIXPANEL_TOKEN=

# Development
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3
REACT_APP_API_CACHE_TTL=300000

# WebSocket Configuration
REACT_APP_WS_RECONNECT_INTERVAL=5000
REACT_APP_WS_MAX_RECONNECT_ATTEMPTS=5
